{
    "presets": [
        [
        "@babel/preset-env",
        {
            "modules": false,
            "targets": {
                "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
            }
        },
        "@babel/preset-stage-2"
        ]
    ],
    "plugins": [
        "@babel/plugin-syntax-dynamic-import",
        "@babel/plugin-transform-runtime",
        "transform-es2015-arrow-functions",
        "@babel/plugin-transform-block-scoping"
    ]
}